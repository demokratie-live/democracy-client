{
  "name": "democracynative",
  "version": "1.5.5",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "ios:prod": "react-native run-ios",
    "ios:internal": "react-native run-ios --scheme=Internal --configuration=Internal.Debug",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:internal": "react-native run-android --variant=internalDebug",
    "test": "jest",
    "lint": "yarn lint:ts && yarn lint:es",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint .",
    "lint:exports": "ts-unused-exports ./tsconfig.json --excludePathsFromReport=__generated__",
    "format": "prettier --check ./src",
    "pods": "cd ios && pod install && cd ..",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@democracy-deutschland/ui": "0.0.5-alpha.7",
    "@react-native-community/async-storage": "1.7.1",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/material-top-tabs": "^6.1.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@types/react": "^17",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-rest": "^0.9.0-rc.1",
    "apollo-link-retry": "^2.2.16",
    "d3-array": "^3.1.1",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.1.0",
    "dateformat": "^5.0.3",
    "graphql": "^16.3.0",
    "graphql-anywhere": "^4.2.7",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lodash.unionby": "^4.8.0",
    "qs": "^6.10.3",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.67.4",
    "react-native-blob-util": "^0.15.0",
    "react-native-camera": "^4.2.1",
    "react-native-device-info": "^8.4.9",
    "react-native-gesture-handler": "^2.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^8.0.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-markdown-view": "https://github.com/Benjamin-Dobell/react-native-markdown-view",
    "react-native-notifications": "^4.2.3",
    "react-native-pager-view": "^5.4.15",
    "react-native-pdf": "^6.5.0",
    "react-native-permissions": "^3.3.0",
    "react-native-progress": "^5.0.0",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-rate": "^1.2.9",
    "react-native-reanimated": "1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.11.1",
    "react-native-sha256": "^1.4.7",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^12.3.0",
    "react-native-tab-view": "^3.1.1",
    "recoil": "^0.7.2",
    "speakingurl": "^14.0.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@graphql-codegen/cli": "2.6.1",
    "@graphql-codegen/fragment-matcher": "3.2.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.10",
    "@graphql-codegen/typescript-document-nodes": "2.2.3",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.1",
    "@graphql-codegen/typescript-operations": "2.3.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.6",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/dateformat": "^5.0.0",
    "@types/detox": "^18.1.0",
    "@types/jasmine": "^3.10.3",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/lodash.unionby": "^4.8.7",
    "@types/react-native": "^0.66.15",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/speakingurl": "^13.0.3",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-import-graphql": "^2.8.1",
    "detox": "^19.4.5",
    "eslint": "^8.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.1.3",
    "jest-circus": "^27.5.1",
    "lint-staged": "^12.3.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^27.1.3",
    "ts-unused-exports": "^8.0.0",
    "typescript": "^4.6.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!(@react-native|react-native|recoil)/)"
  ]
}
