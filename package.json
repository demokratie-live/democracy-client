{
  "name": "democracynative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios:prod": "react-native run-ios",
    "ios:internal": "react-native run-ios --scheme=Internal --configuration=Internal.Debug",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:internal": "react-native run-android --variant=internalDebug",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pods": "cd ios && pod install && cd ..",
    "create-appicon": "yarn create-appicon:production & yarn create-appicon:internal",
    "create-appicon:production": "iconset create ./assets/icons/icon.png",
    "create-appicon:internal": "iconset create --flavor=internal ./assets/icons/icon-internal.png"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "icon-set-creator": "ManAnRuck/icon-set-creator#feature/android-flavor",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
