{
  "name": "@democracy-deutschland/mobile-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --port 8088",
    "ios": "react-native run-ios --port 8088",
    "android": "react-native run-android --port 8088",
    "test": "jest -c jest.config.js",
    "test:watch": "yarn test --watch",
    "device:ios": "react-native run-ios --port 8088 --device",
    "release:ios": "react-native run-ios --configuration Release --port 8088 --device",
    "coverage": "yarn run test --coverage",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx,jsx}' --fix",
    "lint:ts": "tsc -p tsconfig.json --noEmit",
    "lint": "yarn lint:ts && yarn lint:eslint",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "format:check": "prettier -c 'src/**/*.{ts,tsx,js,jsx}'",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "svg-to-jsx": "svgr --config-file ./svgr/svgr.config.js --ext tsx -d ./src/components/Icons ./svgr",
    "pods": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "faker": "^4.1.0",
    "react": "16.9.0",
    "react-art": "^16.11.0",
    "react-native": "0.61.4",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-svg": "^9.13.3",
    "react-native-web": "^0.11.7",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@react-native-community/cli-platform-android": "2.9.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-ondevice-knobs": "^5.2.1",
    "@storybook/addon-options": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@storybook/react-native": "^5.2.1",
    "@storybook/react-native-server": "^5.2.1",
    "@svgr/cli": "^5.0.0",
    "@testing-library/react": "^9.3.2",
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.0.22",
    "@types/react": "16.9.17",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.0",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-jest": "24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "0.56.0",
    "react-dom": "16.9.0",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-testing-library": "^1.11.1",
    "react-native-web-linear-gradient": "^1.0.5",
    "react-test-renderer": "16.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "3.5.2"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
